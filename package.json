    "eslint-plugin-import": "2.26.0",
    "faker": "5.5.3",
    "jest": "28.1.3",
    "lodash": "4.17.21",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4",
    "wait-for-expect": "3.0.2"
